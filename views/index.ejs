<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs')%>

<body>

  <%- include('./partials/nav.ejs')%>
  <div class="blog content">
    <h2>all blogs home folder</h2>
    <% if (blogs && blogs.length> 0) { %>
    <% blogs.forEach(blog=> { %>
    <h3 class="title">
      <%= blog.title %>
    </h3>
    <p class="snippet">
      <%= blog.snippet %>
    </p>
    <% }); %>
    <% } else { %>
    <p>
    <h3>No data found</h3>
    </p>
    <% } %>
  </div>
  <%- include('./partials/footer.ejs')%>
</body>

</html>